{
  "name": "@qws-org/ui-kit",
  "version": "8.0.0",
  "description": "UI Kit for React",
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.es.js",
  "types": "./dist/src/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-preact"
  ],
  "exports": {
    ".": {
      "import": {
          "node": "./dist/index.ssr.js",
          "default": "./dist/index.js"
      },
      "types": "./dist/src/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "type": "module",
  "sideEffects": ["*.css"],
  "scripts": {
    "build:react": "vite build",
    "build": "pnpm prepare:icons && pnpm build:react && UIKIT_SSR_BUILD=true pnpm build:react",
    "storybook": "storybook dev -p 6006 --ci",
    "build-storybook": "pnpm prepare:icons && storybook build --disable-telemetry",
    "prepare:icons": "npx @svgr/cli --typescript --out-dir src/icons -- public/icons",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:ci": "eslint --ignore-pattern .gitignore .",
    "lint:fix": "eslint --fix --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:clear-cache": "rm -rf ./node_modules/.cache/eslint",
    "typecheck": "tsc",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write ."
  },
  "homepage": "https://github.com/qws-org/ui-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/qws-org/ui-kit.git"
  },
  "dependencies": {
    "classnames": "2.5.1",

    "tailwind-variants": "0.3.0"
  },
  "peerDependencies": {
    "react-markdown": "9.0.3",
    "rehype-raw": "7.0.0",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "@react-aria/breadcrumbs": ">=3.5.0",
    "@react-aria/button": ">=3.13.0",
    "@react-aria/checkbox": ">=3.15.0",
    "@react-aria/dialog": ">=3.5.0",
    "@react-aria/disclosure": ">=3.0.0",
    "@react-aria/focus": ">=3.20.0",
    "@react-aria/listbox": ">=3.14.0",
    "@react-aria/menu": ">=3.18.0",
    "@react-aria/overlays": ">=3.27.0",
    "@react-aria/select": ">=3.15.0",
    "@react-aria/separator": ">=3.4.0",
    "@react-aria/table": ">=3.17.0",
    "@react-aria/tabs": ">=3.10.0",
    "@react-aria/textfield": ">=3.17.0",
    "@react-aria/utils": ">=3.28.0",
    "@react-aria/visually-hidden": ">=3.8.0",
    "react-stately": ">=3.40.0",
    "@react-types/shared": ">=3.28.0"
  },
  "devDependencies": {
    "react-markdown": "9.0.3",
